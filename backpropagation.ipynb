      "source": [
        "import torch\n",
        "\n",
        "x = torch.tensor(1.0)\n",
        "y = torch.tensor(2.0)\n",
        "\n",
        "w = torch.tensor(1.0, requires_grad=True)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# forward pass\n",
        "y_hat = w*x\n",
        "loss = ( y_hat - y )**2\n",
        "print(loss)"
      ],
      "metadata": {
